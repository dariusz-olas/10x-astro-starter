{
  "name": "10x-astro-starter",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:logging": "playwright test tests/e2e/logging-and-auth.spec.ts tests/e2e/logs-verification.spec.ts",
    "test:e2e:more-cards": "playwright test tests/e2e/review-more-cards-button.spec.ts",
    "test:e2e:review-full": "playwright test tests/e2e/review-full-flow.spec.ts",
    "test:e2e:verify": "playwright test tests/e2e/logging-and-auth.spec.ts tests/e2e/logs-verification.spec.ts && node scripts/verify-logs-after-tests.js",
    "test:e2e:verify-review": "node scripts/auto-verify-review.js",
    "test:e2e:all": "playwright test",
    "clean:cache": "rm -rf .astro node_modules/.vite dist || rmdir /s /q .astro node_modules\\.vite dist 2>nul || true",
    "clean:all": "npm run clean:cache && rm -rf node_modules package-lock.json || rmdir /s /q node_modules package-lock.json 2>nul || true",
    "logs:clear": "rm -rf logs/*.log || del /q logs\\*.log 2>nul || true",
    "logs:view": "tail -f logs/app-$(date +%Y-%m-%d).log 2>/dev/null || type logs\\app-*.log 2>nul || echo 'No logs found'",
    "logs:error": "tail -f logs/error-$(date +%Y-%m-%d).log 2>/dev/null || type logs\\error-*.log 2>nul || echo 'No error logs found'"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.10",
    "@astrojs/react": "^4.3.1",
    "@astrojs/sitemap": "^3.5.1",
    "@radix-ui/react-slot": "^1.1.2",
    "@supabase/supabase-js": "^2.80.0",
    "@tailwindcss/vite": "^4.1.13",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "astro": "^5.13.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.487.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^3.4.1",
    "tailwind-merge": "^3.1.0",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.2.5"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.7",
    "@eslint/js": "9.23.0",
    "@playwright/test": "^1.56.1",
    "@typescript-eslint/eslint-plugin": "8.28.0",
    "@typescript-eslint/parser": "8.28.0",
    "@vitest/ui": "^4.0.7",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-import-resolver-typescript": "4.2.5",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.5",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-compiler": "19.0.0-beta-aeaed83-20250323",
    "eslint-plugin-react-hooks": "5.2.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.0",
    "prettier-plugin-astro": "0.14.1",
    "typescript-eslint": "8.28.0",
    "vitest": "^4.0.7"
  },
  "lint-staged": {
    "*.{ts,tsx,astro}": [
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
